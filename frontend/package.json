{
	"name": "frontend",
	"private": true,
	"version": "0.0.0",
	"packageManager": "bun@1.3.2",
	"type": "module",
	"scripts": {
		"preinstall": "sh -c 'case ${npm_config_user_agent:-} in npm*) echo \"npm is disabled for this repo. Use bun install.\"; exit 1;; esac'",
		"dev": "bunx --bun vite",
		"build": "bunx --bun tsc -p tsconfig.app.json --noEmit && bunx --bun vite build",
		"lint": "bunx --bun eslint .",
		"preview": "bunx --bun vite preview"
	},
	"dependencies": {
		"@codemirror/commands": "^6.10.0",
		"@codemirror/language": "^6.11.3",
		"@codemirror/state": "^6.5.2",
		"@codemirror/view": "^6.39.0",
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@mui/icons-material": "^7.3.5",
		"@mui/lab": "^7.0.1-beta.19",
		"@mui/material": "^7.3.5",
		"codemirror": "^6.0.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-markdown": "^9.1.0",
		"react-router-dom": "^7.9.5",
		"remark-gfm": "^4.0.1",
		"y-codemirror.next": "^0.3.5",
		"y-websocket": "^1.5.0",
		"yjs": "^13.6.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.1",
		"@types/node": "^24.10.0",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"@vitejs/plugin-react": "^5.1.0",
		"eslint": "^9.39.1",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.24",
		"globals": "^16.5.0",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.46.3",
		"vite": "^7.2.2"
	}
}
